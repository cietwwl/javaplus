<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

	<class name="org.hhhhhh.house.hibernate.dto.HouseDto" table="house">
		<id name="href" column="href" />
		<property name="name" column="name" />
		<property name="floor" column="floor" />
		<property name="plate" column="plate" />
		<property name="price" column="price" />
		<property name="huxing" column="huxing" />
		<property name="is_send_email" column="is_send_email" />
		<property name="tel" column="tel" />
		<property name="owner" column="owner" />
		<property name="commit_date" column="commit_date" />
		<property name="update_date" column="update_date" />
		<property name="zoneName" column="zoneName" />
		<property name="dsc" column="dsc" />
	</class>
	
</hibernate-mapping>
