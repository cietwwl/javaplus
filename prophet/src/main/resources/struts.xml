<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.devMode" value="true" />


	<package name="account4web" extends="struts-default" namespace="/account4web">
		<action name="regist" class="org.hhhhhh.prophet.account4web.RegistAction">
			<result type="redirect">/regist_success.jsp</result>
			<result type="redirect" name="error">/regist.jsp</result>
		</action>
		<action name="login" class="org.hhhhhh.prophet.account4web.LoginAction">
			<result type="redirect">/index.jsp</result>
			<result type="redirect" name="error">/login.jsp</result>
		</action>
		<action name="logout" class="org.hhhhhh.prophet.account4web.LogoutAction">
			<result type="redirect">/index.jsp</result>
		</action>
		<action name="resetPassword" class="org.hhhhhh.prophet.account4web.ResetPasswordAction">
			<result type="redirect">/resetSuccess.jsp</result>
			<result type="redirect" name="error">/resetPassword.jsp</result>
		</action>
		<action name="resetPwd" class="org.hhhhhh.prophet.account4web.ResetPwdAction">
			<result type="redirect">/login.jsp</result>
			<result type="redirect" name="error">/resetPwd.jsp</result>
		</action>
		<action name="getUser" class="org.hhhhhh.prophet.account4web.GetUserAction">
			<result type="redirect">/user.jsp</result>
		</action>
	</package>
	
	<package name="account4app" extends="struts-default" namespace="/account4app">
		<action name="regist" class="org.hhhhhh.prophet.account4app.RegistAction"/>
		<action name="login" class="org.hhhhhh.prophet.account4app.LoginAction"/>
		<action name="resetPwd" class="org.hhhhhh.prophet.account4app.ResetPwdAction"/>
		
		<action name="oneKeyRegist" class="org.hhhhhh.prophet.account4app.OneKeyRegistAction"/>
		<action name="loginById" class="org.hhhhhh.prophet.account4app.LoginByIdAction"/>
		<action name="bindEmail" class="org.hhhhhh.prophet.account4app.BindEmailAction"/>
	</package>
	

	<package name="recharge" extends="struts-default" namespace="/recharge">

		<action name="recharge" class="org.hhhhhh.prophet.recharge.RechargeAction">
			<result type="redirect">/recharge.jsp</result>
			<result type="redirect" name="error">/recharge.jsp</result>
		</action>

		<action name="czdyx" class="org.hhhhhh.prophet.recharge.RechargeCzdyxAction">
			<result type="redirect">/recharge.jsp</result>
			<result type="redirect" name="error">/error.jsp</result>
		</action>

	</package>

	<package name="gm" extends="struts-default" namespace="/gm">

		<action name="login" class="org.hhhhhh.prophet.gm.LoginAction">
			<result type="redirect">index.jsp</result>
			<result type="redirect" name="error">login.jsp</result>
		</action>

		<action name="uploadAction" class="org.hhhhhh.prophet.gm.UploadAction">
			<result type="redirect">/gm/uploadSuccess.jsp</result>
		</action>
		
		
		<action name="fileDownload" class="org.hhhhhh.prophet.gm.FileDownloadAction">  
           <result name="success" type="stream">  
               <param name="contentType">text/plain</param>  
               <param name="contentDisposition">attachment;fileName="game.xml"</param>  
               <param name="inputName">downloadFile</param>  
               <param name="bufferSize">1024</param>  
           </result>  
       </action>  
		
		<action name="uploadClientXmlAction" class="org.hhhhhh.prophet.gm.UploadClientXmlAction">
			<result type="redirect">/gm/uploadSuccess.jsp</result>
		</action>
		
		
		<action name="fileDownloadClientXml" class="org.hhhhhh.prophet.gm.FileDownloadClientXmlAction">  
           <result name="success" type="stream">  
               <param name="contentType">text/plain</param>  
               <param name="contentDisposition">attachment;fileName="XML.zip"</param>  
               <param name="inputName">downloadFile</param>  
               <param name="bufferSize">1024</param>  
           </result>  
       </action>  
		
		<action name="importGmScript" class="org.hhhhhh.prophet.gm.ImportGmScriptAction">
			<result type="redirect">/gm/importSuccess.jsp</result>
		</action>
		
		<action name="importGmScriptText" class="org.hhhhhh.prophet.gm.ImportGmScriptTextAction">
			<result type="redirect">/gm/importSuccess.jsp</result>
		</action>
		
		<action name="getGmScript" class="org.hhhhhh.prophet.gm.GetGmScriptAction"/>
		
		
       		
		<action name="zipFileDownload" class="org.hhhhhh.prophet.gm.ZipFileDownloadAction" >  
           <result name="success" type="stream">  
               <param name="contentType">text/plain</param>  
               <param name="contentDisposition">attachment;fileName="game.zip"</param>  
               <param name="inputName">downloadFile</param>  
               <param name="bufferSize">1024</param>  
           </result>  
       </action>  

		<action name="createProject" class="org.hhhhhh.prophet.gm.CreateProjectAction">
			<result type="redirect">/gm/index.jsp</result>
		</action>
		<action name="selectProject" class="org.hhhhhh.prophet.gm.SelectProjectAction">
			<result type="redirect">/gm/zones.jsp</result>
		</action>
		<action name="deleteProject" class="org.hhhhhh.prophet.gm.DeleteProjectAction">
			<result type="redirect">/gm/index.jsp</result>
		</action>

		<action name="createZone" class="org.hhhhhh.prophet.gm.CreateZoneAction">
			<result type="redirect">/gm/zones.jsp</result>
		</action>

		<action name="deleteZone" class="org.hhhhhh.prophet.gm.DeleteZoneAction">
			<result type="redirect">/gm/zones.jsp</result>
		</action>

		<action name="selectZone" class="org.hhhhhh.prophet.gm.SelectZoneAction">
			<result type="redirect">/gm/zone.jsp</result>
		</action>

		<action name="saveSystemProperty" class="org.hhhhhh.prophet.gm.SaveSystemPropertiesAction">
			<result type="redirect">/gm/zoneBody.jsp</result>
		</action>

		<action name="deleteSystemProperty" class="org.hhhhhh.prophet.gm.DeleteSystemPropertyAction">
			<result type="redirect">/gm/zoneBody.jsp</result>
		</action>

		<action name="setZoneName" class="org.hhhhhh.prophet.gm.SetZoneNameAction">
			<result type="redirect">/gm/zoneBody.jsp</result>
		</action>
		
		<action name="copyProperties" class="org.hhhhhh.prophet.gm.CopyPropertiesAction">
			<result type="redirect">/gm/zoneBody.jsp</result>
		</action>

	
		
	</package>



</struts>    
